{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "application.ts",
        "config/index.ts",
        "config/vars.ts",
        "getApplication.ts",
        "getK8sRuntime.ts",
        "getReleaseChannel.ts",
        "index.ts",
        "k8sRuntime.ts",
        "managedK8sRuntime.ts",
        "provider.ts",
        "releaseChannel.ts",
        "runtimeLink.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
